def convert_value(value, scale=10):
	convert_val = []
	for val in value:
		if val > 0x8000:
			val = val - 0x10000
			convert_val.append(val / scale)
		else:
			convert_val.append(val / scale)
	return convert_val

def from_modbus_register(value, scale=10):
	if value > 0x8000:
		value = value - 0x10000
	converted_value = value / scale
	return converted_value

def to_modbus_register(value, scale=10):
    scaled_int = int(round(value * scale))
    register_value = scaled_int & 0xFFFF
    return register_value


